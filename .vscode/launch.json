            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/benchmark",
            "args": ["--settings", "benchmark.json"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/benchmark",
            "environment": [],
